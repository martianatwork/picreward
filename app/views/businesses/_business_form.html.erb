<%= simple_form_for(@business) do |f| %>
  <%= f.error_notification %>

  <%= f.input :name %>
  <%= f.input :adress %>
  <%= f.input :category %>
  <%= f.input :tva %>
  <%= f.input :contact_name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.button :submit %>
<% end %>
