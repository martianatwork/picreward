<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "<i class='fa fa-instagram'></i> Sign in with #{OmniAuth::Utils.camelize(provider)}".html_safe, omniauth_authorize_path(resource_name, provider), class:"btn btn-block btn-social btn-instagram" %><br />
  <% end -%>
<% end -%>
